{
  "name": "openweather-client",
  "version": "0.0.5",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production tsc -b && node ./dist/src/bin/www",
    "dev": "cross-env NODE_ENV=development nodemon --watch \"**/*.ts\" --exec ts-node -r tsconfig-paths/register ./src/bin/www.ts",
